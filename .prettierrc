{
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf",
    "overrides": [
        {
            "files": ["*.scss", "*.css"],
            "options": {
                "printWidth": 80,
                "tabWidth": 2,
                "useTabs": false,
                "singleQuote": false,
                "semi": true
            }
        }
    ]
}